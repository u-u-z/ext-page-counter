{
  "name": "Page Counter",
  "description": "A browser plugin for tracking the number of page views for the day.",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_title": "Click Me",
    "default_icon": {
      "32": "images/logo.png"
    }
  },
  "icons": {
    "32": "images/logo.png"
  }
}